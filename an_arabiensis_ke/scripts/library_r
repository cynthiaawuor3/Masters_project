#Loading WGCNA
library(WGCNA)
library(tidyverse)     # tidyverse will pull in ggplot2, readr, other useful libraries
library(magrittr)      # provides the %>% operator
library(ggplot2)
library(readxl)
library(DESeq2)
library(gplots)
library("flashClust")
library("igraph")
library(edgeR)
library(readr)


# ensure results are reproducible
set.seed(1)


# other settings
options(digits = 4)
options(stringsAsFactors = FALSE)

# set number of threads to allow in WGCNA analysis
allowWGCNAThreads(nThreads=4)

